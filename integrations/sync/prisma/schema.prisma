datasource db {
  provider = "sqlite"
  url      = "file:./sna-sync.db"
}

generator client {
  provider = "prisma-client-js"
}

model User {
  id Int @id @default(autoincrement())

  steamId   String @unique
  discordId String @unique
  cadUserId String @unique
  apiToken  String

  @@unique([steamId, discordId, cadUserId])
}
