  },
  "license": "MIT",
  "scripts": {
    "format:quick": "pretty-quick --staged --pattern \"packages/**/*.{js,jsx,ts,tsx,md,css,json}\"",
    "format": "prettier --write \"**/*.{js,mjs,jsx,json,ts,tsx,yml,md}\"",
    "lint": "eslint . --ext .ts,.tsx,.mjs",
    "prepare": "husky install",
    "postinstall": "husky install",
    "build": "npm run build:nui && node scripts/build-integrations.mjs",
    "build:nui": "cd integrations/sync/nui-dev && npm run build",
    "build:zip": "node scripts/zip-integrations.mjs",
    "dev": "nodemon --config nodemon.json"
  },
  "devDependencies": {
    "@casper124578/eslint-config": "^6.0.0",
    "@citizenfx/client": "^2.0.6613-1",
    "@citizenfx/server": "^2.0.6594-1",
    "@types/archiver": "^5.3.2",
    "@types/node": "^20.4.7",
    "@types/ws": "^8.5.5",
    "@typescript-eslint/parser": "^6.2.1",
    "esbuild": "^0.18.17",
    "eslint": "^8.46.0",
    "eslint-config-prettier": "^8.10.0",
    "eslint-plugin-prettier": "^5.0.0",
    "husky": "^8.0.3",
    "nodemon": "^3.0.1",
    "prettier": "^3.0.1",
    "typescript": "^5.1.6"
  },
  "dependencies": {
    "@octokit/action": "^6.0.5",
    "archiver": "^5.3.1",
    "dotenv": "^16.3.1",
    "undici": "^5.23.0"
  },
  "prettier": {
    "semi": true,
    "trailingComma": "all",
    "singleQuote": false,
    "printWidth": 100,
    "tabWidth": 2
  }
}
