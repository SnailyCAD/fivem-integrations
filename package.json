    "@types/node": "^18.14.2",
    "@types/ws": "^8.5.4",
    "@typescript-eslint/parser": "^5.54.0",
    "esbuild": "^0.17.10",
    "eslint": "^8.35.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.3",
    "prettier": "^2.8.4",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "@octokit/action": "^5.0.2",
    "archiver": "^5.3.1",
    "undici": "^5.22.1"
